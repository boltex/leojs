            'helpPanelWebview',
            'LeoJS Help',
            { viewColumn: vscode.ViewColumn.Beside, preserveFocus: true },
            {
                enableScripts: true,
            }
        );
        this._context.subscriptions.push(this._helpPanel);
        this._helpPanel.onDidDispose(
