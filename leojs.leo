            vscode.workspace.registerFileSystemProvider(
                Constants.URI_LEOJS_SCHEME,
                this._leoFileSystem,
                { isCaseSensitive: true }
            )
        );
        this._bodyFileSystemStarted = true;
    }

    this.loadSearchSettings();
}

