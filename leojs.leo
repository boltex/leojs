<t tx="felix.20221027153815.1">public widget_name(w: any): string {
    let name: string;
    if (!w) {
        name = '&lt;no widget&gt;';
    } else if (w['getName']) {
        name = w.getName();
    } else if (w['objectName']) {
        name = w.objectName();
    } else if (w['_name']) {
        name = w._name;
    } else if (w['name']) {
        name = w.name;
    } else {
        name = w.toString();
    }
    return name;
}

