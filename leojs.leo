public setNewBodyUriTime(p_uri: vscode.Uri, v: VNode): void {
    const w_gnx = utils.leoUriToStr(p_uri);
    this._setOpenedBodyTime(w_gnx);
    this.openedBodiesVNodes[w_gnx] = v;
}

